 <!-- ==================== Airframe ==================== -->

 <animation>
  <type>select</type>
  <object-name>Propeller</object-name>
  <condition>
   <less-than>
    <property>engines/engine[0]/rpm</property>
    <value>400</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Propeller.Slow</object-name>
  <nopreview/>
  <condition>
   <and>
    <greater-than>
     <property>engines/engine[0]/rpm</property>
     <value>300</value>
    </greater-than>
    <less-than>
     <property>engines/engine[0]/rpm</property>
     <value>1400</value>
    </less-than>
   </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Propeller.Fast</object-name>
  <nopreview/>
  <condition>
   <greater-than>
    <property>engines/engine[0]/rpm</property>
    <value>1400</value>
   </greater-than>
  </condition>
 </animation>

