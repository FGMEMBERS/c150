      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>2.40</x-m>
      <y-m>-0.30</y-m>
      <z-m>-0.540</z-m>
    </center>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>RWheelGroup</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <factor>9.0</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>2.40</x-m>
      <y-m>0.30</y-m>
      <z-m>-0.540</z-m>
    </center>
  </animation>
  
 <animation>
  <type>rotate</type>
  <object-name>LeftAileron</object-name>
  <property>surface-positions/left-aileron-pos-norm</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>14</dep>
