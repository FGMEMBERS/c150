    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.972</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.730</z-m>
    </center>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>FUdamper</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <factor>90</factor>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.860</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.852</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LWheelGroup</object-name>
    <property>gear/gear[1]/compression-norm</property>
