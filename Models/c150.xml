 <!--  -->

 <animation>
  <type>rotate</type>
  <object-name>NoseStrutGroup</object-name>
  <condition>
   <property>gear/gear[0]/wow</property>
  </condition>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>30.0</factor>
  <axis>
   <x1-m>0.84</x1-m>
   <y1-m>0</y1-m>
   <z1-m>-0.54</z1-m>
   <x2-m>0.78</x2-m>
   <y2-m>0</y2-m>
   <z2-m>-1.09</z2-m>
  </axis>
 </animation>

 <animation>
    <type>translate</type>
    <object-name>NoseWheelPart</object-name>
    <object-name>NoseWheel</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <factor>0.5</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>FLdamper</object-name>
    <property>gear/gear[0]/compression-norm</property>
