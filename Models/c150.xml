<?xml version="1.0"?>

<PropertyList>

 <path>cessna150.ac</path>


 <panel>
  <name>MainPanel</name>
  <path>Aircraft/c150/Panels/c150l-panel.xml</path>
  <bottom-left>
   <x-m> 1.665</x-m>
   <y-m>-0.475</y-m>
   <z-m>-0.165</z-m>
  </bottom-left>
  <bottom-right>
   <x-m> 1.665</x-m>
   <y-m> 0.50</y-m>
   <z-m>-0.165</z-m>
  </bottom-right>
  <top-left>
   <x-m> 1.665</x-m>
   <y-m>-0.475</y-m>
   <z-m> 0.272</z-m>
  </top-left>
  <depth-test type="bool">true</depth-test>
 </panel>

 <!-- LOD for the model as a whole -->

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10000</max-m>
 </animation>

 <!-- move some objects to bottom of aircraft graph -->
 <animation>
  <object-name>bottom</object-name>
  <object-name>WindShield</object-name>
  <object-name>Propeller.Slow</object-name>
  <object-name>Propeller.Fast</object-name>
 </animation>

 <animation>
  <type>noshadow</type>
  <object-name>WindShield</object-name>
  <object-name>WindowsBack</object-name>
  <object-name>Propeller.Fast</object-name>
  <object-name>switchGroup</object-name>
  <object-name>cabin</object-name>
  <object-name>ground</object-name>
 </animation>

 <animation>
  <type>noshadow</type>
  <object-name>Ldoorglass</object-name>
 </animation>

 <animation>
  <type>noshadow</type>
  <object-name>Rdoorglass</object-name>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Ring</object-name>
  <property>/instrumentation/magnetic-compass/indicated-heading-deg</property>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
  <center>
   <x-m>1.627</x-m>
   <y-m>0.0</y-m>
   <z-m>0.547</z-m>
  </center>
 </animation>


 <!-- move the 2D panel on top of aircraft graph -->
 <animation>
  <object-name>top</object-name>
  <object-name>MainPanel</object-name>
 </animation>

<!-- Controls -->

 
 <animation>
  <name>CarbHeatGroup</name>
  <object-name>CarbHeat</object-name>
  <object-name>CarbHeatBack</object-name>
  <object-name>CarbHeatRod</object-name>
