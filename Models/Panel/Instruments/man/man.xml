<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>manifold-gauge.ac</path>
  
 <animation>
  <type>scale</type>
  <x-offset>0.7</x-offset>
  <y-offset>0.7</y-offset>
  <z-offset>0.7</z-offset>
 </animation>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
            <emission>
            <red>0.028</red>
            <green>0.014</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine/mp-osi</property>
    <interpolation>
        <entry><ind>  0 </ind><dep>   50 </dep></entry>
        <entry><ind> 10 </ind><dep>  58 </dep></entry>
        <entry><ind> 30 </ind><dep> 165 </dep></entry>
        <entry><ind> 50 </ind><dep> 270 </dep></entry>
        <entry><ind> 70 </ind><dep> 375 </dep></entry>
    </interpolation>
    <axis>
        <x> -1 </x>
        <y>  0 </y>
        <z>  0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>

</PropertyList>
